<div class="row">
  <div class="col-lg-12">
    <div class="container">
      <h1>Alderney Marine Management Plan</h1>
      <h6> <%= @page.content(:text).try(:html_safe) %> </h6>

      <table class="table table-striped">
        <thead>
          <tr>
            <th>Date</th>
            <th>Stage</th>
          </tr>
        </thead>
        <tbody>
          <% @page.children.last(8).each do |child| %>
            <tr>
              <td><%= child.content(:date).try(:html_safe) %></td>
              <td><%= child.content(:text).try(:html_safe) %></td>
            </tr>
          <% end %>
        </tbody>
      </table>
    </div>
  </div>
</div>
